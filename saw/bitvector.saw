let bitvector_t_and_spec (n : Int) = do {
  (px, x_nBits, x_nLength, x_nLength_max, x_List, x_pList, x_nResizeAmt) <- bitvector_t n "x";
  bitvector_t_preconditions n x_nBits x_nLength x_nLength_max x_nResizeAmt;

  (py, y_nBits, y_nLength, y_nLength_max, y_List, y_pList, y_nResizeAmt) <- bitvector_t n "y";
  bitvector_t_preconditions n y_nBits y_nLength y_nLength_max y_nResizeAmt;

  crucible_execute_func [px, py];

  (x_nBits', x_nLength', x_nLength_max', x_List', x_pList', x_nResizeAmt') <- bitvector_t' n "x'" px;
  bitvector_t_postconditions n x_nBits' x_nLength' x_nLength_max' x_nResizeAmt';

  crucible_postcond {{ x_List' == x_List }};
  
  (y_nBits', y_nLength', y_nLength_max', y_List', y_pList', y_nResizeAmt') <- bitvector_t' n "y'" py;
  bitvector_t_postconditions n y_nBits' y_nLength' y_nLength_max' y_nResizeAmt';

  crucible_postcond {{ y_List' == y_List }};

  (presult, r_nBits, r_nLength, r_nLength_max, r_List, r_pList, r_nResizeAmt) <- bitvector_t n "r";
  bitvector_t_postconditions n r_nBits r_nLength r_nLength_max r_nResizeAmt;

  crucible_return presult;

  crucible_postcond {{ r_List == x_List && y_List }};
};

