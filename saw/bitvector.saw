let bitvector_t_fromHexString_spec (n : Int) = do {
  (string, pstring) <- ptr_to_fresh "string" (llvm_array n (llvm_int 8));

  crucible_precond {{ [ (c >= 'a' /\ c <= 'f') \/
                        (c >= 'A' /\ c <= 'F') \/
			(c >= '0' /\ c <= '9')
	              | c <- string ] == ~zero }};

  crucible_execute_func [pstring, crucible_term {{ `n : [64] }}];

  (pbv, nBits', nLength', nLength_max', List', pList', nResizeAmt') <-
    bitvector_t (eval_int {{ `(n*4) : [32] }}) "bv";
  
  crucible_return pbv;

  bitvector_t_postconditions (eval_int {{ `(n*4) : [32] }}) nBits' nLength' nLength_max' nResizeAmt';
  crucible_postcond {{ hexstring_to_bv string == fromList List' }};
};

let bitvector_t_copyUpdate_spec (n : Int) = do {
  (pdst, d_nBits, d_nLength, d_nLength_max, d_List, d_pList, d_nResizeAmt) <- bitvector_t n "dst";
  bitvector_t_preconditions n d_nBits d_nLength d_nLength_max d_nResizeAmt;

  (psrc, s_nBits, s_nLength, s_nLength_max, s_List, s_pList, s_nResizeAmt) <- bitvector_t n "src";
  bitvector_t_preconditions n s_nBits s_nLength s_nLength_max s_nResizeAmt;

  crucible_execute_func [pdst, psrc];

  (d_nBits', d_nLength', d_nLength_max', d_List', d_pList', d_nResizeAmt') <- bitvector_t' n "dst'" pdst;
  bitvector_t_postconditions n d_nBits' d_nLength' d_nLength_max' d_nResizeAmt';

  (s_nBits', s_nLength', s_nLength_max', s_List', s_pList', s_nResizeAmt') <- bitvector_t' n "src'" psrc;
  bitvector_t_postconditions n s_nBits' s_nLength' s_nLength_max' s_nResizeAmt';

  crucible_postcond {{ s_List == d_List' }};
};

