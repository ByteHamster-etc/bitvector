  bitvector_t_preconditions n nBits nLength nLength_max nResizeAmt;

  crucible_execute_func [pslice, pbv, crucible_term {{ `b0 : [32] }}, crucible_term {{ `b1 : [32] }}];

  (nBits', nLength', nLength_max', List', pList', nResizeAmt') <- bitvector_t' n "bv'" pbv;
  bitvector_t_postconditions n nBits' nLength' nLength_max' nResizeAmt';

  crucible_postcond {{ List == List' }};

  (s_nBits', s_nLength', s_nLength_max', s_List', s_pList', s_nResizeAmt') <- bitvector_t' nSliceLength "slice'" pslice;
  bitvector_t_postconditions nSliceLength s_nBits' s_nLength' s_nLength_max' s_nResizeAmt';

  crucible_postcond {{ fromList s_List' == (fromList`{b=n} List)!!([b1, b1-1 .. b0]:[_][32]) }};
};

