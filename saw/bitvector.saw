  crucible_execute_func [pbv];

  (nBits', nLength', nLength_max', List', pList', nResizeAmt') <- bitvector_t' n "bv'" pbv;

  bitvector_t_postconditions n nBits' nLength' nLength_max' nResizeAmt';
